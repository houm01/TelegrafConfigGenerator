[[inputs.snmp]]
  agents = ["udp://{{ instance['ip'] }}:{{instance['port']}}"]
  version = 3
  timeout = "{{ config['inputs'][instance['auth']]['timeout'] }}"
  retries = 3
  path = ["/usr/share/snmp/mibs"]
  sec_name = "{{ config['inputs'][instance['auth']]['sec_name'] }}"
  auth_protocol = "{{ config['inputs'][instance['auth']]['auth_protocol'] }}"
  auth_password = "{{ config['inputs'][instance['auth']]['auth_password'] }}"
  priv_protocol = "{{ config['inputs'][instance['auth']]['priv_protocol'] }}"
  priv_password = "{{ config['inputs'][instance['auth']]['priv_password'] }}"
  sec_level = "{{ config['inputs'][instance['auth']]['sec_level'] }}"
